{
  "name": "GrowingIO-Track",
  "id": "GrowingIO-Track",
  "version": "2.9.13",
  "description": "GrowingIO埋点SDK",
  "_dp_type": "nativeplugin",
  "_dp_nativeplugin":{
    "android": {
      "plugins": [
        {
          "type": "module",
          "name": "GrowingIO-Track",
          "class": "com.growingio.android.uniplugin.GrowingIOTrackModule"
        }
      ],
      "hooksClass": "com.growingio.android.uniplugin.GrowingIOTrackConfigAppProxy",
      "integrateType": "aar",
      "dependencies": [
        "com.growingio.android:vds-android-agent:track-2.9.13"
      ],
      "minSdkVersion": "17",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.READ_PHONE_STATE"
      ],
      "parameters": {
        "growing_android_account_id": {
          "des": "项目ID(必填字段)",
          "key": "growing_android_account_id",
          "value": "${growing_android_account_id}"
        },
        "growing_android_url_scheme": {
          "des": "urlScheme(必填字段, 同时需要在 App常用其它设置->Android设置->UrlSchemes 中设置)",
          "key": "growing_android_url_scheme",
          "value": "${growing_android_url_scheme}"
        },
        "growing_android_debug": {
          "des": "是否开启android debug(可选字段(true or false), 默认为false)",
          "key": "growing_android_debug",
          "value": "${growing_android_debug}"
        },
        "growing_channel": {
          "des": "设置渠道(可选字段)",
          "key": "growing_channel",
          "value": "${growing_channel}"
        },
        "growing_zone": {
          "des": "设置时区(可选字段)",
          "key": "growing_zone",
          "value": "${growing_zone}"
        },
        "growing_gta_host": {
          "des": "设置数据后台服务器地址(可选字段)",
          "key": "growing_gta_host",
          "value": "${growing_gta_host}"
        },
        "growing_data_host": {
          "des": "设置数据查看平台服务器地址(可选字段)",
          "key": "growing_data_host",
          "value": "${growing_data_host}"
        },
        "growing_report_host": {
          "des": "设置设备报活服务器地址(可选字段)",
          "key": "growing_report_host",
          "value": "${growing_report_host}"
        },
        "growing_ws_host": {
          "des": "设置WebSocket地址(可选字段)",
          "key": "growing_ws_host",
          "value": "${growing_ws_host}"
        },
        "growing_tracker_rnmode": {
          "des": "设置rn模式(可选字段)",
          "key": "growing_tracker_rnmode",
          "value": "${growing_ws_host}"
        },
        "growing_tracker_host": {
          "des": "设置数据收集平台服务器地址(可选字段)",
          "key": "growing_tracker_host",
          "value": "${growing_tracker_host}"
        },
        "growing_android_delay_init": {
          "des": "是否延迟初始化(可选字段(true or false), 默认为false)",
          "key": "growing_android_delay_init",
          "value": "${growing_android_delay_init}"
        },
        "growing_android_data_collect": {
          "des": "是否开启数据采集(可选字段(true or false), 默认为true)",
          "key": "growing_android_delay_init",
          "value": "${growing_android_delay_init}"
        },
        "growing_android_id_enable": {
          "des": "是否采集androidid(可选字段(true or false), 默认为true)",
          "key": "growing_android_id_enable",
          "value": "${growing_android_id_enable}"
        },
        "growing_android_imei_enable": {
          "des": "是否采集imei(可选字段(true or false), 默认为true)",
          "key": "growing_android_imei_enable",
          "value": "${growing_android_imei_enable}"
        },
        "growing_android_oaid_enable": {
          "des": "是否采集oaid(可选字段(true or false), 默认为true)",
          "key": "growing_android_oaid_enable",
          "value": "${growing_android_oaid_enable}"
        },
        "growing_android_read_clip_board_enable": {
          "des": "是否读取剪切板(可选字段(true or false), 默认为true), 该功能涉及延迟深度链接是否可用",
          "key": "growing_android_read_clip_board_enable",
          "value": "${growing_android_read_clip_board_enable}"
        }
      }
    },
    "ios": {
      "plugins": [{
        "type": "module",
        "name": "GrowingIO-Track",
        "class": "GrowingIOTrackModule"
      }],
      "integrateType": "framework",
      "hooksClass": "GrowingIOTrackConfigProxy",
      "frameworks": ["Foundation.Framework", "Security.framework", "CoreTelephony.framework",
        "SystemConfiguration.framework", "AdSupport.framework", "libicucore.tbd", "libsqlite3.tbd",
        "CoreLocation.framework", "JavaScriptCore.framework", "WebKit.framework"],
      "deploymentTarget": "8.0",
      "parameters": {
        "growing_ios_account_id": {
          "des": "项目ID(必填字段)",
          "key": "growing_ios_account_id"
        },
        "growing_ios_debug": {
          "des": "是否开启ios debug(可选字段(YES or NO), 默认为NO)",
          "key": "growing_ios_debug"
        },
        "growing_zone": {
          "des": "设置时区(可选字段)",
          "key": "growing_zone"
        },
        "growing_gta_host": {
          "des": "设置数据后台服务器地址(可选字段)",
          "key": "growing_gta_host"
        },
        "growing_data_host": {
          "des": "设置数据查看平台服务器地址(可选字段)",
          "key": "growing_data_host"
        },
        "growing_report_host": {
          "des": "设置设备报活服务器地址(可选字段)",
          "key": "growing_zone"
        },
        "growing_ws_host": {
          "des": "设置WebSocket地址(可选字段)",
          "key": "growing_ws_host"
        },
        "growing_tracker_host": {
          "des": "设置数据收集平台服务器地址(可选字段)",
          "key": "growing_tracker_host"
        },
        "growing_ios_delay_init": {
          "des": "是否延迟初始化(可选字段(YES or NO), 默认为NO)",
          "key": "growing_ios_delay_init"
        },
        "growing_ios_read_clip_board_enable": {
          "des": "是否读取剪切板(可选字段(YES or NO), 默认为YES), 该功能涉及延迟深度链接是否可用",
          "key": "growing_ios_read_clip_board_enable"
        }
      }
    }
  }
}
